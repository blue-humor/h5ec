{
  "private": true,
  "scripts": {
    "start": "cross-env UMI_ENV=dev umi dev",
    "build": "umi build",
    "build:pro": " cross-env UMI_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.0",
    "@nutui/nutui-react": "^1.4.1",
    "@react-vant/icons": "^0.0.10",
    "cross-env": "^7.0.3",
    "nprogress": "^0.2.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.1.5",
    "react": "17.x",
    "react-dom": "17.x",
    "react-transition-group": "^4.4.5",
    "react-vant": "^3.0.4",
    "swiper": "^8.4.5",
    "umi": "^3.5.35",
    "vconsole": "^3.15.0",
    "weixin-js-sdk": "^1.6.0",
    "xgplayer": "^2.32.2"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.35",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
